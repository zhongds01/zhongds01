<?xml version="1.0" encoding="GB2312"?>
<ucmframe name="服务框架">
   <configitem name = "SYSTEM" desc = "开通框架配置">
     <item name = "DEBUG"        desc = "实时网元调试">true</item>
     <item name = "FILESYSTEM"   desc = "映射文件是否为文件系统">false</item>
     <item name = "REGION_ID"    desc = "服务开通中心地市路由[暂时未用]">REGION_ID</item>
     <item name = "KERNELSYSTEM" desc = "模板映射转换类">com.asiainfo.sacenter.shcmcc.receive.dbcp.impl.ISystemKernelImpl</item>
     <item name = "LOCALE" desc = "语言属性设置">
        <item name = "LOCALE"    desc = "国际化">CHINESE</item>
        <item name = "LOCALE_FIXED"  desc = "固定国际化(不采用web或客户端传入的语言)">true</item>
        <item name = "LOCALE_CUSTOM" desc="自定义语言资源实现类" >com.ai.sacenter.i18n.impl.SFUpdfxResourceImpl</item>
     </item>
     <item name = "CONTRO"  desc = "核心控制类">
        <item name = "SUBFLOW"      desc = "服务控制类">
           <item name = "ROLLBACK"       desc = "是否支持回滚流程">true</item>
           <item name = "RBWFQFOM"       desc = "是否回滚到调度作业规则">false</item>
           <item name = "ESWFHUMA"       desc = "服务容错补偿是否落地规则">false</item>
        </item>
        <item name = "RSGMSYSTEM"   desc = "采用号段局向(TRUE资源号段局向)规则">false</item>
        <item name = "HUMAN"        desc = "是否网元异常落地捕获规则">true</item>
        <item name = "REDO_TIMES"   desc = "通默认重做次数规则">5</item>
        <item name = "INTERVAL"     desc = "通默认重做时间间隔(秒)">600</item>
        <item name = "REGION_ID"    desc = "默认地市编码(开通消息非法存储地市)">200</item>
        <item name = "PROGRAM"      desc = "服务定单默认组件">CMKTC_ROCKET_COMPLETE_PROGRAM</item>
     </item>
   </configitem>
   <module file="com/ai/sacenter/module/sacenter-core-frame.module"      name="服务开通核心服务模块" use="true"/>
   <module file="com/ai/sacenter/receive/sacenter-receive-custom.module" name="服务开通定单接收模块" use="true"/>
</ucmframe>
