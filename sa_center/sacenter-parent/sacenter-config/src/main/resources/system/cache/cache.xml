   <!-- BatchIdGenerator关系 -->
   <cache source="DB" dataType="DATABASE" id="com.ai.appframe2.complex.cache.impl.BatchIdGeneratorCacheImpl"/>
   <cache source="DB" dataType="DATABASE" id="com.ai.appframe2.complex.cache.impl.SecAllAccessCacheImpl" init="false"/>
