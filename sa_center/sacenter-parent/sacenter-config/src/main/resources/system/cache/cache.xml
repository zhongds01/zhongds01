      <property name="table" value="BS_STATIC_DATA"/>
   </cache>
   <!-- 分表函数关系 -->
   <cache source="DB" dataType="DATABASE" id="com.ai.appframe2.complex.cache.impl.TableSplitMappingCacheImpl">
      <property name="table" value="CFG_TABLE_SPLIT_MAPPING"/>
   </cache>
