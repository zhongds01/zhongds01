<?xml version="1.0" encoding="GB2312"?>
<ucmframe id = "DsmpMapping" desc = "DSMP测产品模块">
   <mappings id = "IUsrm2GernL"   desc="全量用户状态模块">
      <mapping name = "L_SO_NBR"        mapping = "SO_NBR"           method = "MappingContext"  expr = "IInstanceL.VM_ORDER_ID"    			desc = "用户状态变更流水号"/>
      <mapping name = "L_USER_STS"      mapping = "USER_STS"         method = "MappingContext"  expr = "VMICMOSO,IUsm2UpdomL.OBLIGATE1"     desc = "用户状态(00正常01单停02停机03预销04销户05过户06改号)"/>
      <mapping name = "L_USER_ID"       mapping = "USER_ID"          method = "MappingContext"  expr = "IInstanceL.USER_ID"        			desc = "用户id"/>
      <mapping name = "L_BILL_ID"       mapping = "BILL_ID"          method = "MappingContext"  expr = "BILL_ID,IInstanceL.BILL_ID"        	desc = "手机号"/>
      <mapping name = "L_OLD_BILL_ID"   mapping = "OLD_BILL_ID"      method = "MappingContext"  expr = "OLD_BILL_ID"        	            desc = "旧手机号"/>
      <mapping name = "L_REGION_CODE"   mapping = "REGION_CODE"      method = "MappingCenter"   desc = "地市代码"/>
      <mapping name = "L_USER_ID"       mapping = "TRANSACT_ID"      method = "MappingContext"  expr = "IInstanceL.USER_ID"        			desc = "用户id"/>
      <mapping name = "L_COMMIT_DATE"   mapping = "COMMIT_DATE"      method = "MappingSysdate"  desc = "数据插入时间"/>
      <mapping name = "L_PROC_STS"      mapping = "STS"              method = "MappingDirect"   expr = "1"                         			desc = "数据状态"/>
      <mapping name = "OPRTIME"         mapping = "STS_CHG_TIME"     method = "MappingSysdate"  desc = "状态变化时间"/>
      <mapping name = "LOprTime"        mapping = "STS_OPR_TIME"     method = "MappingSysdate"  desc = "状态操作时间"/>
   </mappings>                                                                                                                              			
                                                                                                                                            			
   <mappings id = "IUorm2GernL"   desc="平台用户订购模块">                                                                                  			
      <mapping name = "L_SO_NBR"        mapping = "SO_NBR"           method = "MappingGenSID"   expr = "I_OPEN_RADIUS_INDEX"       			desc = "订购流水号"/>
      <mapping name = "L_MSO_NBR"       mapping = "MSO_NBR"          method = "MappingDirect"   expr = "0"                         			desc = "批次订购流水号"/>
      <mapping name = "L_BILL_ID"       mapping = "BILL_ID"          method = "MappingContext"  expr = "BILL_ID,IInstanceL.BILL_ID"        	desc = "手机号"/>
      <mapping name = "L_OLD_BILL_ID"   mapping = "OLD_BILL_ID"      method = "MappingContext"  expr = "OLD_BILL_ID"        	            desc = "旧手机号"/>
      <mapping name = "L_REGION_CODE"   mapping = "REGION_CODE"      method = "MappingCenter"   desc = "地市代码"/>
      <mapping name = "VMICMSPID"       mapping = "SP_CODE"          method = "MappingDirect"   desc = "SP代码"/>
      <mapping name = "VMICMBIZCODE"    mapping = "OPERATOR_CODE"    method = "MappingDirect"   desc = "SP业务代码"/>
      <mapping name = "VMICMBLFLG"      mapping = "BILL_FLAG"        method = "MappingDirect"   desc = "SP计费类型"/>
      <mapping name = "L_ACT_TYPE"      mapping = "ACT_TYPE"         method = "MappingContext"  expr = "IUsm2UpdomL.PS_SERVICE"    			desc = "操作类型"/>
      <mapping name = "VMICMTDK"        mapping = "BRAND"            method = "MappingSplite"   expr = "GOTONE=0;SZX=1;CTK=1;DZK=1;MZONE=2;*=3"   desc = "用户品牌(0全球通1神州行2动感地带3其它省内品牌)"/>
      <mapping name = "L_USER_STS"      mapping = "USER_STS"         method = "MappingContext"  expr = "VMICMOSO,IUsm2UpdomL.OBLIGATE1"     desc = "用户状态(00正常01单停02停机03预销04销户05过户06改号)"/>
      <mapping name = "L_USER_ID"       mapping = "TRANSACT_ID"      method = "MappingContext"  expr = "IInstanceL.USER_ID"         		desc = "用户id"/>
      <mapping name = "L_COMMIT_DATE"   mapping = "COMMIT_DATE"      method = "MappingSysdate"  desc = "数据插入时间"/>
      <mapping name = "L_PROC_STS"      mapping = "STS"              method = "MappingDirect"   expr = "1"                          		esc = "数据状态"/>
      <mapping name = "L_OPRSRC"        mapping = "SRC"              method = "MappingVeloci"   expr = "$IUcm2KernL.getVMISrc($IInstanceL)" desc = "受理渠道"/>
      <mapping name = "OPRCMSGT"        mapping = "PKG_SEQ"          method = "MappingDirect"   desc = "VGOP批量包编号"/>
      <mapping name = "OPRCNUMB"        mapping = "SINGLE_SEQ"       method = "MappingDirect"   desc = "VGOP批量包序号"/>
      <mapping name = "L_ORDER_FLAG"    mapping = "ORDER_FLAG"       method = "MappingDirect"   expr = "0"                         			desc = "是否被动"/>
      <mapping name = "L_ORDER_ID"      mapping = "EXT2"             method = "MappingContext"  expr = "IInstanceL.VM_ORDER_ID"    			desc = "数据插入批次号"/>
  </mappings>
  
</ucmframe>
