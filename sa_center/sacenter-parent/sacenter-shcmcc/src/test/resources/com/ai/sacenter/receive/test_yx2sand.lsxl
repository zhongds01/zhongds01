--退订
busiCode=YX_2_SAND_0001;BusiType=11;strOriginBillId=${PROVISION.BILL_ID};strOriginIccid=${SIMICCID};strOriginSn=#{SIMICCSN};strOperSerial=${PROVISION.PS_ID};strOperTime=#{OPRTIME}
