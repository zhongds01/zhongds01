<?xml version="1.0" encoding="UTF-8"?>
<sysbolist>
  <sysbo datasource="so1" name="IOpenDsmpStatus" mainattr="SO_NBR">
    <mapingenty type="table">{I_OPEN_DSMP_STATUS}</mapingenty>
    <datafilter />
    <attrlist>
      <attr floatlength="0" name="SO_NBR" maxlength="15" datatype="Long" type="PK" remark="用户状态变更流水号">
        <mapingcol datatype="NUMBER">SO_NBR</mapingcol>
      </attr>
      <attr floatlength="0" name="USER_STS" maxlength="2" datatype="String" type="COL" remark="用户状态按照规范定义
00	正常
01	单向停机
02	停机
03	预销户
04	销户
05	过户
06	换号">
        <mapingcol datatype="VARCHAR2">USER_STS</mapingcol>
      </attr>
      <attr floatlength="0" name="BILL_ID" maxlength="32" datatype="String" type="COL" remark="手机号">
        <mapingcol datatype="VARCHAR2">BILL_ID</mapingcol>
      </attr>
      <attr floatlength="0" name="OLD_BILL_ID" maxlength="32" datatype="String" type="COL" remark="原手机号（换号使用）">
        <mapingcol datatype="VARCHAR2">OLD_BILL_ID</mapingcol>
      </attr>
      <attr floatlength="0" name="REGION_CODE" maxlength="4" datatype="Integer" type="COL" remark="地市代码">
        <mapingcol datatype="NUMBER">REGION_CODE</mapingcol>
      </attr>
      <attr floatlength="0" name="ORG_ID" maxlength="8" datatype="Long" type="COL" remark="局向编码">
        <mapingcol datatype="NUMBER">ORG_ID</mapingcol>
      </attr>
      <attr floatlength="0" name="USER_ID" maxlength="12" datatype="Long" type="COL" remark="用户id">
        <mapingcol datatype="NUMBER">USER_ID</mapingcol>
      </attr>
      <attr floatlength="0" name="CUST_ID" maxlength="12" datatype="Long" type="COL" remark="客户id">
        <mapingcol datatype="NUMBER">CUST_ID</mapingcol>
      </attr>
      <attr floatlength="0" name="ACCT_ID" maxlength="12" datatype="Long" type="COL" remark="帐户id">
        <mapingcol datatype="NUMBER">ACCT_ID</mapingcol>
      </attr>
      <attr floatlength="0" name="TRANSACT_ID" maxlength="15" datatype="Long" type="COL" remark="交易批次号">
        <mapingcol datatype="NUMBER">TRANSACT_ID</mapingcol>
      </attr>
      <attr floatlength="0" name="COMMIT_DATE" maxlength="20" datatype="DateTime" type="COL" remark="数据插入时间">
        <mapingcol datatype="DATE">COMMIT_DATE</mapingcol>
      </attr>
      <attr floatlength="0" name="STS_CHG_TIME" maxlength="20" datatype="DateTime" type="COL" remark="状态变更时间">
        <mapingcol datatype="DATE">STS_CHG_TIME</mapingcol>
      </attr>
      <attr floatlength="0" name="STS_OPR_TIME" maxlength="20" datatype="DateTime" type="COL" remark="状态操作时间">
        <mapingcol datatype="DATE">STS_OPR_TIME</mapingcol>
      </attr>
      <attr floatlength="0" name="DONE_CODE" maxlength="14" datatype="Long" type="COL" remark="DSMP处理流水号">
        <mapingcol datatype="NUMBER">DONE_CODE</mapingcol>
      </attr>
      <attr floatlength="0" name="DONE_DATE" maxlength="20" datatype="DateTime" type="COL" remark="DSMP处理日期">
        <mapingcol datatype="DATE">DONE_DATE</mapingcol>
      </attr>
      <attr floatlength="0" name="STS" maxlength="2" datatype="Integer" type="COL" remark="DSMP处理状态；初始-1，已经发送到MISC-2；已经发送到中心-3；处理失败-0">
        <mapingcol datatype="NUMBER">STS</mapingcol>
      </attr>
      <attr floatlength="0" name="REMARK" maxlength="255" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">REMARK</mapingcol>
      </attr>
      <attr floatlength="0" name="REV1" maxlength="32" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">REV1</mapingcol>
      </attr>
      <attr floatlength="0" name="REV2" maxlength="20" datatype="Long" type="COL">
        <mapingcol datatype="NUMBER">REV2</mapingcol>
      </attr>
      <attr floatlength="0" name="REV3" maxlength="64" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">REV3</mapingcol>
      </attr>
    </attrlist>
    <relationlist />
    <oplist />
  </sysbo>
</sysbolist>
