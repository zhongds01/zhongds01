<?xml version="1.0" encoding="UTF-8"?>
<sysbolist>
  <sysbo datasource="so1" name="IOpenDsmpOrder" mainattr="SO_NBR">
    <mapingenty type="table">{I_OPEN_DSMP_ORDER}</mapingenty>
    <datafilter />
    <attrlist>
      <attr floatlength="0" name="SO_NBR" maxlength="15" datatype="Long" type="PK">
        <mapingcol datatype="NUMBER">SO_NBR</mapingcol>
      </attr>
      <attr floatlength="0" name="MSO_NBR" maxlength="15" datatype="Long" type="COL" remark="MSO_NBR和SO_NBR不同时MSO_NBR是主工单(批量交易工单号)">
        <mapingcol datatype="NUMBER">MSO_NBR</mapingcol>
      </attr>
      <attr floatlength="0" name="REGION_CODE" maxlength="4" datatype="Integer" type="COL">
        <mapingcol datatype="NUMBER">REGION_CODE</mapingcol>
      </attr>
      <attr floatlength="0" name="SP_CODE" maxlength="20" datatype="String" type="COL" remark="SP_CODE
">
        <mapingcol datatype="VARCHAR2">SP_CODE</mapingcol>
      </attr>
      <attr floatlength="0" name="OPERATOR_CODE" maxlength="20" datatype="String" type="COL" remark="SP业务编码">
        <mapingcol datatype="VARCHAR2">OPERATOR_CODE</mapingcol>
      </attr>
      <attr floatlength="0" name="ACT_TYPE" maxlength="2" datatype="String" type="COL" remark="操作类型
06定购07退订04暂停05恢复">
        <mapingcol datatype="VARCHAR2">ACT_TYPE</mapingcol>
      </attr>
      <attr floatlength="0" name="BRAND" maxlength="6" datatype="Integer" type="COL" remark="当无品牌信息时,为0">
        <mapingcol datatype="NUMBER">BRAND</mapingcol>
      </attr>
      <attr floatlength="0" name="USER_STS" maxlength="2" datatype="String" type="COL" remark="记录用户的在网状态
">
        <mapingcol datatype="VARCHAR2">USER_STS</mapingcol>
      </attr>
      <attr floatlength="0" name="BILL_ID" maxlength="50" datatype="String" type="COL" remark="msisdn">
        <mapingcol datatype="VARCHAR2">BILL_ID</mapingcol>
      </attr>
      <attr floatlength="0" name="OLD_BILL_ID" maxlength="50" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">OLD_BILL_ID</mapingcol>
      </attr>
      <attr floatlength="0" name="TRANSACT_ID" maxlength="15" datatype="Long" type="COL" remark="交易批次">
        <mapingcol datatype="NUMBER">TRANSACT_ID</mapingcol>
      </attr>
      <attr floatlength="0" name="COMMIT_DATE" maxlength="20" datatype="DateTime" type="COL" remark="数据入表时间">
        <mapingcol datatype="DATE">COMMIT_DATE</mapingcol>
      </attr>
      <attr floatlength="0" name="PROCESS_DATE" maxlength="20" datatype="DateTime" type="COL" remark="CBOSS填写">
        <mapingcol datatype="DATE">PROCESS_DATE</mapingcol>
      </attr>
      <attr floatlength="0" name="STS" maxlength="2" datatype="Integer" type="COL" remark="1:初始（BOSS插入数据时的状态）
0:失败（DSMP失败时状态）
2:处理中（CBOSS将数据取走时的状态）
3:成功（DSMP失败时状态）
4:部分成功（DSMP部分成功时态，暂时没有用）
5:超时(DSMP超时) &apos;;">
        <mapingcol datatype="NUMBER">STS</mapingcol>
      </attr>
      <attr floatlength="0" name="PKG_SEQ" maxlength="64" datatype="String" type="COL" remark="平台下发批量交易的流水号">
        <mapingcol datatype="VARCHAR2">PKG_SEQ</mapingcol>
      </attr>
      <attr floatlength="0" name="SINGLE_SEQ" maxlength="64" datatype="String" type="COL" remark="平台下发批量交易中每个单条交易信息的流水号，或单个业务中的流水号">
        <mapingcol datatype="VARCHAR2">SINGLE_SEQ</mapingcol>
      </attr>
      <attr floatlength="0" name="SRC" maxlength="4" datatype="Integer" type="COL">
        <mapingcol datatype="NUMBER">SRC</mapingcol>
      </attr>
      <attr floatlength="0" name="RECONFIRM_FLAG" maxlength="2" datatype="Integer" type="COL" remark="0-不需要二次确认
1-需要二次确认">
        <mapingcol datatype="NUMBER">RECONFIRM_FLAG</mapingcol>
      </attr>
      <attr floatlength="0" name="RECONFIRM_CODE" maxlength="64" datatype="String" type="COL" remark="1-用户回复“是”；
2-用户回复“否”；
3-用户超时没有回复">
        <mapingcol datatype="VARCHAR2">RECONFIRM_CODE</mapingcol>
      </attr>
      <attr floatlength="0" name="ORDER_FLAG" maxlength="2" datatype="Integer" type="COL" remark="0-订购关系变化引起的订购关系；
1-用户状态变化引起的订购关系；">
        <mapingcol datatype="NUMBER">ORDER_FLAG</mapingcol>
      </attr>
      <attr floatlength="0" name="RESULT" maxlength="16" datatype="String" type="COL" remark="BOSS发起业务不填，平台发起填写交易处理结果返回码">
        <mapingcol datatype="VARCHAR2">RESULT</mapingcol>
      </attr>
      <attr floatlength="0" name="RESULT_DESC" maxlength="255" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">RESULT_DESC</mapingcol>
      </attr>
      <attr floatlength="0" name="REMARK" maxlength="255" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">REMARK</mapingcol>
      </attr>
      <attr floatlength="0" name="EXT1" maxlength="32" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">EXT1</mapingcol>
      </attr>
      <attr floatlength="0" name="EXT2" maxlength="32" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">EXT2</mapingcol>
      </attr>
      <attr floatlength="0" name="EXT3" maxlength="64" datatype="String" type="COL">
        <mapingcol datatype="VARCHAR2">EXT3</mapingcol>
      </attr>
      <attr floatlength="0" name="BILL_FLAG" maxlength="4" datatype="Integer" type="COL" remark="计费类型
1免费2按次/按条计费3包月计费5包次计费7包天计费9按栏目包月计费11时长计费12包周（仅在各省站使用）21按月计费">
        <mapingcol datatype="NUMBER">BILL_FLAG</mapingcol>
      </attr>
    </attrlist>
    <relationlist />
    <oplist />
  </sysbo>
</sysbolist>
